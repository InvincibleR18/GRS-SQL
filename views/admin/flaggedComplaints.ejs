<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Admin Flagged Complaints</title>
    <link href="https://maxcdn.bootstrapcdn.com/bootstrap/4.1.1/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">    
	<link rel="stylesheet" href="/css/cards.css">
    <link rel="stylesheet" href="/css/navbarActive.css">   
    <script src="https://kit.fontawesome.com/6d1a88741d.js" crossorigin="anonymous"></script>
</head>
<body>
<%-include('../partials/navbarAdmin.ejs')%>

<!-- Flagged Complaints (Status=2) -->
<div class="team-boxed">
    <div class="container">
        <div class="intro">
            <h2 class="text-center">Flagged Complaints</h2>
        </div>
        <div class="row people">
            <% for(let complaint of complaints){ %>
				<% if(complaint._status!=2) continue;%>
                <div class="col-md-6 col-lg-4 item">
                    <div class="box"><img class="rounded-circle" src="data:image/png;base64,">
						<h3 class="name"><%=complaint.title%></h3>
                        <p class="title"><%=complaint.dept%> Department</p>
                        <p class="description">Issued By <%= complaint.userName %></p>
                        <p class="description">Area Code: <%=complaint.areaCode%></p>
                        <div class="d-flex flex-row justify-content-center">
                            <form target="_blank" action="/admin/complaint/<%=complaint.ID%>" method="GET" class="p-2">
                                <button class="btn btn-success">View</button>
                            </form>
                        </div>
                    </div>
                </div>
            <%  }  %>
        </div>
    </div>
</div>

<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.1.1/js/bootstrap.min.js"></script>
</body>
</html>
